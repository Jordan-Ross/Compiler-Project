program bitwise is
    integer a;
    bool b;
begin
    a := 4;
    b := true;

    putbool(not a); // (bitwise) true, NOT (boolean) false 
    putchar('_');//delimiter

    putbool(not b); // (boolean) false, NOT (bitwise) true
    putchar('_');//delimiter

    putbool(8 & true); // (boolean) true & true == true, NOT (bitwise) 8 & 1 == 0
    putchar('_');//delimiter

    putinteger(8 & true); // (bitwise) 8 & 1 == 0
    putchar('_');//delimiter
end program.

