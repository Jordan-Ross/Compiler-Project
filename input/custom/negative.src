program negative is 
    integer ai;
    float af;
begin
    ai := -1;
    putinteger(ai);//-1
    putchar('_');
    ai := -ai;
    putinteger(ai);//+1
    putchar('_');
    ai := - ai;
    putinteger(ai);//-1
    putchar('_');
    af := -1;
    putfloat(af);//-1
    putchar('_');
    af := -1.3;
    putfloat(af);//-1.3
    putchar('_');
    af := -af;
    putfloat(af);//+1.3
    
end program
