program asdf is 
begin
    putbool(1 < 2); // true
    putbool(1 < 0); // false 

    putbool(1 > 0); // true
    putbool(1 > 2); // false 

    putbool(1 == 1); // true
    putbool(1 == 2); // false

    putbool(1 != 2); // true
    putbool(1 != 1); // false

    putbool(1 >= 1); // true
    putbool(1 >= 0); // true
